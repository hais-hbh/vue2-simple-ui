    "@vue/component-compiler-utils": "^3.3.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "karma": "^6.3.3",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^3.1.0",
